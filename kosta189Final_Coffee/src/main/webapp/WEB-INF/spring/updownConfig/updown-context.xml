<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 파일 업로드를 위한 설정 id는 multipartResolver로 약속되어 있다! -->
	<bean
		class="org.springframework.web.multipart.commons.CommonsMultipartResolver"
		id="multipartResolver" />


	<!-- 다운로드를 위한 view Resolver - 당장필요없어서 주석처리 혹시 다운로드 기능이 포함되면 주석해제 <bean class 
		= "org.springframework.web.servlet.view.BeanNameViewResolver"> <property 
		name="order" value="1"/> </bean> 
		-->
</beans>
